{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "overrides": [
    {
      "files": [
        "**/*.mjs"
      ],
      "options": {
        "singleQuote": true
      }
    },
    {
      "files": [
        "**/*.yml"
      ],
      "options": {
        "singleQuote": false
      }
    }
  ]
}